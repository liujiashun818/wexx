<template name="star">
    <view class="star">
        <block wx:for="{{stars}}" wx:for-item="i">
        <image wx:if="{{i}}" src="/images/star.png"></image>
        <image wx:else src="/images/none-star.png"></image>
         </block>
        <text class="star-average">{{average}}</text>
       
    </view>
</template>